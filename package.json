{
  "name": "react-db",
  "private": true,
  "version": "0.0.0",
  "main": "server.ts",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "lint": "npx eslint src",
    "dev:server": "nodemon --watch server.ts --watch src/server --exec \"ts-node server.ts\"",
    "dev:client": "npm run build:client && vite --config vite.config.ts dev",
    "build": "rimraf dist && tsc -p tsconfig.prod.json && npm run build:client && npm run build:server && npm run copy-files",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "typecheck": "tsc --noEmit",
    "serve": "npm run build && cross-env NODE_ENV=production npm ts-node ./server",
    "serve:local": "vite serve",
    "clean": "rimraf dist/",
    "serv": "ts-node server.ts",
    "copy-files": "copyfiles static/* dist/assets && copyfiles index.html dist && copyfiles -f dist/client/assets/* dist/assets",
    "start": "vite",
    "cy:open-e2e": "cypress open --e2e --browser chrome",
    "cy:run-e2e": "cypress run --e2e",
    "cypress": "start-server-and-test start http://localhost:3000/ cy:run-e2e && npx nyc report --reporter=text-summary",
    "cy:e2e": "start-server-and-test start http-get://localhost:3000 cy:open-e2e"
  },
  "dependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@reduxjs/toolkit": "^1.9.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "axios": "^1.3.5",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "cypress": "^12.10.0",
    "express": "^4.18.2",
    "msw": "^1.2.1",
    "nodemon": "^2.0.22",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.8",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "react-spinners": "^0.13.8",
    "rimraf": "^5.0.0",
    "sass": "^1.59.3",
    "serve-static": "^1.15.0",
    "ts-node": "^10.9.1",
    "vite": "^4.1.4",
    "vite-plugin-istanbul": "^4.0.1",
    "vitest": "^0.29.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/express": "^4.17.17",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "@vitest/coverage-c8": "^0.29.2",
    "c8": "^7.13.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "jsdom": "^21.1.1",
    "lint-staged": "^12.4.1",
    "prettier": "^2.8.4",
    "start-server-and-test": "^2.0.0",
    "typescript": "^4.9.5"
  }
}
